{
  "name": "SeguClick",
  "description": "Herramientas para el Asesor de Seguros",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["contextMenus"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "images/icon.png"
  },
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "logo": "images/logo.png"
}